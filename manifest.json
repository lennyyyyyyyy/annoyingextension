{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "main.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>",
                "*://*/*",
                "https://www.youtube.com/*",
                "https://www.reddit.com/*",
                "https://www.tiktok.com/*",
                "https://www.google.com/*"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ]
}