{
    "manifest_version": 3,
    "name": "Annoying Extension (go outside)",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "main.html",
        "default_icon": "newicon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ],
    "background": {
        "service_worker": "service_worker.js"
    },
    "permissions": [
        "storage"
    ]
}